		"@types/uuid": "^8.3.1",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"@vitejs/plugin-legacy": "^1.6.2",
		"@vitejs/plugin-vue": "^1.9.3",
		"@vue/eslint-config-typescript": "8.0.0",
